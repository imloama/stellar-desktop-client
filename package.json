{
  "name": "polarstar",
  "version": "1.0.0",
  "author": "imloama@gmail.com",
  "description": "stellar desktop wallet",
  "github": "https://github.com/imloama/stellar-desktop-client",
  "license": "MIT",
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node .electron-vue/build.js && electron-builder",
    "build:dir": "node .electron-vue/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node .electron-vue/build.js",
    "build:web": "cross-env BUILD_TARGET=web node .electron-vue/build.js",
    "dev": "node .electron-vue/dev-runner.js",
    "watch": "cross-env BUILD_TARGET=web node .electron-vue/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config .electron-vue/webpack.renderer.config.js",
    "postinstall": "",
    "pbuild": "node .electron-vue/pbuild.js",
    "pbuild:darwin": "cross-env BUILD_TARGET=darwin BUILD_ARCH=x64 node .electron-vue/pbuild.js",
    "pbuild:linux": "cross-env BUILD_TARGET=linux BUILD_ARCH=x64 node .electron-vue/pbuild.js",
    "pbuild:linux32": "cross-env BUILD_TARGET=linux BUILD_ARCH=ia32 node .electron-vue/pbuild.js",
    "pbuild:mas": "cross-env BUILD_TARGET=mas node .electron-vue/pbuild.js",
    "pbuild:win32": "cross-env BUILD_TARGET=win32 BUILD_ARCH=ia32  node .electron-vue/pbuild.js",
    "pbuild:win64": "cross-env BUILD_TARGET=win32 BUILD_ARCH=x64 node .electron-vue/pbuild.js"
  },
  "build": {
    "productName": "polarstar",
    "appId": "com.github.imloama.polarstar",
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "animate.css": ">3.5.2",
    "axios": ">0.17.1",
    "better-scroll": "^1.4.2",
    "bip39": "^2.4.0",
    "create-hmac": "^1.1.6",
    "decimal.js": ">=9.0.1",
    "echarts": ">=4.0.1",
    "ed25519": ">=0.0.4",
    "electron-log": "^2.2.17",
    "js-nacl": "^1.2.2",
    "localforage": "^1.7.2",
    "lodash": ">=4.17.4",
    "moment": ">=2.20.1",
    "nedb": "^1.8.0",
    "nprogress": ">=0.2.0",
    "number-precision": ">=1.2.0",
    "raf.js": ">=0.0.4",
    "shvl": ">=1.2.0",
    "stellar-hd-wallet": ">=0.0.4",
    "stellar-sdk": "^8.2.3",
    "swiper": ">=4.0.7",
    "update-electron-app": "^1.3.0",
    "uuid": ">=3.1.0",
    "vue": "^2.4.2",
    "vue-awesome-swiper": ">=2.6.7",
    "vue-i18n": ">=7.2.0",
    "vue-methods-promise": ">=0.2.2",
    "vue-pull-refresh": ">=0.2.6",
    "vue-qr": ">=1.2.3",
    "vue-router": ">=2.7.0",
    "vue-toasted": ">=1.1.21",
    "vuedraggable": ">=2.16.0",
    "vuetify": "1.0.0",
    "vuex": ">=2.4.0",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.2",
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-add-filehash": "^6.9.4",
    "babel-plugin-equire": "^1.1.0",
    "babel-plugin-transform-imports": "^1.4.1",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-minify": "^0.4.3",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "babili-webpack-plugin": "^0.1.2",
    "base64-font-loader": "^0.0.4",
    "cfonts": "^1.1.3",
    "chalk": "^2.1.0",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "cssnano": "^3.10.0",
    "del": "^3.0.0",
    "devtron": "^1.4.0",
    "electron": "^3.0.0",
    "electron-builder": "^19.19.1",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.2.0",
    "electron-packager": "^12.1.0",
    "electron-rebuild": "^1.1.3",
    "es6-promise": "^4.1.1",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "friendly-errors-webpack-plugin": ">=1.1.3",
    "hammerjs": ">=2.0.8",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy-middleware": "^0.17.3",
    "js-base64": ">=2.3.2",
    "json-loader": ">=0.5.4",
    "multispinner": "^0.2.1",
    "node-loader": "^0.6.0",
    "npm": ">=5.5.1",
    "opn": ">=5.1.0",
    "optimize-css-assets-webpack-plugin": "^2.0.0",
    "ora": ">=1.2.0",
    "requires-port": ">=1.0.0",
    "rimraf": ">=2.6.0",
    "semver": ">=5.3.0",
    "shelljs": ">=0.7.6",
    "sjcl": ">=1.0.7",
    "style-loader": "^0.18.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "url-loader": "^0.5.9",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^13.0.5",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.2",
    "webpack-dev-server": "^2.7.1",
    "webpack-hot-middleware": "^2.18.2"
  }
}
